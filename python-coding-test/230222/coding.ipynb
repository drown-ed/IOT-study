   "source": [
    "# 2018 two-pointer\n",
    "\n",
    "num = int(input())\n",
    "\n",
    "start, end, cnt, sum = 1, 1, 1, 1\n",
    "\n",
    "while end != num:\n",
    "    if sum == num:\n",
    "        cnt += 1\n",
    "        end += 1 \n",
    "        sum += end\n",
    "    elif sum > num:\n",
    "        sum -= start \n",
    "        start += 1\n",
    "    else:\n",
    "        end += 1\n",
    "        sum += end \n",
    "\n",
    "print(cnt)"
   ]
  }
 ],
